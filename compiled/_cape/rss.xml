    <item>
      <title>Projects</title>
      <link>http://rwdfruhoh.ookb.co/_cape/projects.mustache</link>
      <description>&lt;!DOCTYPE html&gt;
&lt;html lang="en"&gt;
  &lt;head&gt;
    &lt;title&gt;RWDF Projects&lt;/title&gt;
    &lt;meta name="viewport" content="width=device-width, initial-scale=1.0"&gt;
    &lt;!-- Bootstrap --&gt;
    &lt;link rel="stylesheet" href="//netdna.bootstrapcdn.com/bootstrap/3.0.0/css/bootstrap.min.css" media="screen"&gt;
    &lt;link href="//netdna.bootstrapcdn.com/font-awesome/3.2.1/css/font-awesome.css" rel="stylesheet"&gt;
    &lt;link href="http://cape.ookb.co/css/rwdffeed.css" rel="stylesheet"&gt;
  &lt;/head&gt;
&lt;!-- count:  --&gt;
  &lt;body&gt;

    &lt;header class="container"&gt;
      &lt;img class="logo pull-right" src="http://rwdfruhoh.ookb.co/assets/media/rwdflogo.jpg"/&gt;
      &lt;h1 class=""&gt;Deutsch Foundation projects&lt;/h1&gt;
      &lt;div class="row"&gt;
        &lt;p class="col-6 col-sm-12"&gt;Please click the &lt;i class="icon-thumbs-up"&gt;&lt;/i&gt; to feature a project on the homepage. Click the &lt;i class="icon-ban-circle"&gt;&lt;/i&gt; to send to the unused feed.&lt;/p&gt;
      &lt;/div&gt;
    &lt;/header&gt;

    &lt;div class="container"&gt;

      &lt;div class="row"&gt;

          &lt;!-- LISTING OF RWDF PROJECTS --&gt;
          &lt;div class="col-12 col-sm-6 col-med-4 col-lg-3 cape-picker block"&gt;

            &lt;h3&gt;&lt;/h3&gt;
            &lt;small&gt;&lt;/small&gt;


            &lt;div class="choose"&gt;
              &lt;!-- CAPE Promote/Demote buttons --&gt;
                &lt;a href="//removed" class="remove" title="REMOVE"&gt;
                  &lt;i class="icon-ban-circle"&gt;&lt;/i&gt;
                &lt;/a&gt;
                &lt;a href="//added" class="add" title="ADD"&gt;
                  &lt;i class="icon-thumbs-up"&gt;&lt;/i&gt;
                &lt;/a&gt;
            &lt;/div&gt;
          &lt;/div&gt;

      &lt;/div&gt;

    &lt;/div&gt;

    &lt;!-- JavaScript plugins (requires jQuery) --&gt;
    &lt;script src="http://code.jquery.com/jquery.js"&gt;&lt;/script&gt;
    &lt;!-- Include all compiled plugins (below), or include individual files as needed --&gt;
    &lt;script src="//netdna.bootstrapcdn.com/bootstrap/3.0.0/js/bootstrap.min.js"&gt;&lt;/script&gt;

    &lt;script&gt;
      $(document).ready(function() {
        var list_type = ''
        $("a.remove").click(function(e) {
          var url = $(this).attr('href');
          $(this).parent().parent().remove();
          $.get(url);
          return false;
        });
        $("a.add").click(function(e) {
          var url = $(this).attr('href');
          $(this).parent().parent().remove();
          $.get(url);
          return false;
        });
      });
    &lt;/script&gt;

    &lt;!-- make all the bios the same height with JS, not CSS --&gt;
    &lt;!-- from here: http://stackoverflow.com/questions/6781031/use-jquery-css-to-find-the-tallest-of-all-elements --&gt;
    &lt;script&gt;
      $(document).ready(function() {
        var maxHeight = -1;
        $('.block').each(function() {
          maxHeight = maxHeight &gt; $(this).height() ? maxHeight : $(this).height();
        });
        $('.block').each(function() {
          $(this).height(maxHeight);
        });
      });
    &lt;/script&gt;

  &lt;!-- ! COMPILED TIME  !--&gt;
  &lt;/body&gt;
&lt;/html&gt;
</description>
